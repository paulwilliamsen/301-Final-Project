<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
  </head>
  <header>
  <%- include('../partials/header') %>
  </header>
  <body>

      
      <form action="/eventData" method="get">
        <button type="submit">View Events</button>
      </form>

    <div class="dashboard">
    <!-- <%- include('../partials/location', {location}) %> -->
    <h2 class="location-name"><%= location.formatted_query %></h2>

    <form action="/location" method="post">
      <input type="text" name="search" required/>
      <button type="submit">New Search</button>
    </form>

    <p><%= weather[0].time %></p>
    <p><%= weather[0].forecast %></p>

    <%= news[0].name%>
    <%= news[0].author%>
    <%= news[0].title%>
    <%= news[0].description%>
    <%= news[0].url%>
    <%= news[0].publishedAt%>
    </div>  
    
  </body>
</html>

